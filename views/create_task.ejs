            display: flex;
            position: relative;
            align-items: center;
        }

        label {
            margin-right: 10px; 
            font-weight: bold;
            color: #333;
            width: 150px;
        }

        input, textarea, select {
            flex: 1;
            padding: 10px;
            border: 1px solid #ccc;
            border-radius: 6px;
            font-size: 16px;
            box-sizing: border-box;    
            max-width: 300px;
        }
        .space{
            width: 100%;
            height: 30px;
            background: rgb(14, 14, 46);
        }
        button, .upl {
            padding: 10px 20px;
            background-color: #007bff;
            color: #fff;
            border: none;
            border-radius: 6px;
            cursor: pointer;
            font-size: 16px;
            margin-top: 10px;
            text-decoration: none;
            text-align: center;
        }
        h4{
            text-align: center;
            color: black;
            font-size: 4vh;
        }
        button:hover, .upl:hover {
            background-color: #0056b3;
        }

        .back-link {
            text-decoration: none;
            color: white;
            background-color: #007bff;
            padding: 10px 15px;
            border-radius: 5px;
            transition: background-color 0.3s;
            display: inline-block;
        }

        .back-link:hover {
            background-color: #fff;
            color: rgb(43, 41, 41);
        }
        .btnss{
            display: flex;
            align-items: center;
            justify-content: center;
        }
        label {
            margin-right: 10px;
            font-weight: bold;
            color: #333;
             }
        .lab{
            display: flex;
            white-space: nowrap;  
            align-items: flex-start;
            margin: 12px;
            gap: 32px;
        }
        .finp{
            width: 60px;
             }
        .checkbox-group {
           width: 100%;
        }

@media (max-width: 768px) {
   .lab{
    flex-direction: column;
   }
} 
        @media (max-width: 768px) {
